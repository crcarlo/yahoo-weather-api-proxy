{
	"version": 2,
	"name": "yahoo-weather-api-proxy",
	"builds": [{ "src": "*.js", "use": "@now/node" }],
	"env": {
		"YAHOO_APP_ID": "@yahoo-app-id",
		"YAHOO_CONSUMER_KEY": "@yahoo-consumer-key",
		"YAHOO_CONSUMER_SECRET": "@yahoo-consumer-secret"
	},
	"routes": [
		{ "src": "/(.*)", "dest": "/index.js" }
	]
}
